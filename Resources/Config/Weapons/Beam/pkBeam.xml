<?xml version="1.0"?>
<weapon>
	<!-- full name -->
	<name>Photokinetic Beam</name>
	<!-- short name -->
	<shortName>PKBEAM 2</shortName>
	<!-- file name -->
	<fileName>pkBeam</fileName>
	<!-- name of sound made when fired -->
	<fireSound>ShotC</fireSound>
	<!-- weapon class -->
	<class>beam</class>
	<!-- beam length -->
	<length>30</length>
	<!-- life, in seconds -->
	<life>0.175</life>
	<!-- damage dealt -->
	<damage>50</damage>
	<!-- cooldown time, in miliseconds -->
	<cooldown>0.125</cooldown>
	<!-- cost, in energy -->
	<energyCost>5</energyCost>
	<!-- velocity -->
	<velocity>1050</velocity>
	<!-- energy usage -->
	<energy>10</energy>
</weapon>