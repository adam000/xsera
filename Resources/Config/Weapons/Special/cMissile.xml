<?xml version="1.0" ?>
<weapon>
	<!-- full name -->
	<name>Concussion Missile</name>
	<!-- sprite name -->
	<sprite>Weapons/cMissile</sprite>
	<!-- short name -->
	<shortName>cMissile</shortName>
	<!-- weapon class -->
	<class>special</class>
	<!-- name of sound made when fired -->
	<fireSound>RocketLaunchr</fireSound> <!-- This file is not original -->
	<!-- mass, in kg -->
	<mass>5000</mass>
	<!-- turning rate, in radians/second -->
	<turnrate>1.5</turnrate>
	<!-- maximum seeking angle -->
	<maxSeek>0.6</maxSeek>
	<!-- thrust, in newtons -->
	<thrust>4500000</thrust>
	<!-- life, in miliseconds -->
	<life>1500</life>
	<!-- damage dealt -->
	<damage>100</damage>
	<!-- cooldown time, in miliseconds -->
	<cooldown>1200</cooldown>
	<!-- cost, in energy -->
	<energyCost>100</energyCost>
	<!-- ammo -->
	<ammo>5</ammo>
</weapon>