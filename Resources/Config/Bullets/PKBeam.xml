<?xml version="1.0"?>
<sprite>
	<!-- full name -->
	<name>Photokinetic Beam</name>
	<!-- short name -->
	<shortName>PKBeam</shortName>
	<!-- name of sound made when fired -->
	<fireSound>ShotC</fireSound>
	<!-- life, in miliseconds -->
	<life>175</life>
	<!-- damage dealt -->
	<damage>50</damage>
	<!-- cooldown time, in miliseconds -->
	<cooldown>150</cooldown>
	<!-- cost, in energy -->
	<energyCost>5</energyCost>
	<!-- velocity -->
	<velocity>1050</velocity>
	<!-- weapon class -->
	<class>beam</class>
	<!-- beam length -->
	<length>30</length>
</sprite>
