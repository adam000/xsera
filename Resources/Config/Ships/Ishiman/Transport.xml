<?xml version="1.0" ?>
<ship>
	<!-- full name -->
	<name>Transport</name>
	<!-- short name -->
	<shortname>TRNSPT</shortname>
	<!-- sprite name -->
	<sprite>Ishiman/Transport</sprite>
	<!-- amount of health -->
	<life>250</life>
	<!-- mass, in kg -->
	<mass>300000</mass>
	<!-- turning rate, in radians/second -->
	<turnrate>1.0</turnrate>
	<!-- energy (in energy units?) -->
